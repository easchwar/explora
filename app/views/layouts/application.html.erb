<!DOCTYPE html>
<html>
<head>
  <title>Explora</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

  <% if current_user %>
    <script charset="utf-8">
      window.CURRENT_USER = {
        id: <%= current_user.id %>,
        username: "<%= current_user.username %>",
      };
    </script>
  <% end %>
</head>
<body>

<%= yield %>

</body>
</html>
